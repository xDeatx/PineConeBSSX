  
    local deb = false
    local UserInputService = game:GetService("UserInputService")
    local convert = game.Workspace:FindFirstChild("Convert")
    local player = game:GetService("Players").LocalPlayer
    local AutoConvertV = false
    local AutoDigV = false
    local MultiDigV = false

    convert.CanCollide = false
    convert.Transparency = 1

    -- Initalize Turtle Library
    
    local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Turtle-Brand/Turtle-Lib/main/source.lua"))()

    -- Create a window

    local args = {
        [1] = false
    }


    local window = library:Window("Pinecone v0.2")

    window:Toggle("Auto Dig", false, function(bool)
                if AutoDigV == false then
                    AutoDigV = bool

                    print("Auto Dig Enabled.")
                    
                    while task.wait(0.2) do
                        player.Character:FindFirstChildOfClass("Tool"):WaitForChild(ToolRemote):FireServer(unpack(args))
                    end

                else
                    print("Auto Dig Disabled")
                    AutoDigV = false
                end
        end)

        


    window:Toggle("Auto Convert", false, function(bool)

        if AutoConvertV == false then
                    AutoConvertV = bool
                    print("Auto-Convert Enabled")
                    while AutoConvertV == true do
                        convert.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
                        wait(0.1)
                        convert.Position = Vector3.new(50, 1000, 50)
                    end
                else
                    print("Auto-Convert Disabled")
                    AutoConvertV = false
                end

    end)



    window:Toggle("Multi Dig", false, function(bool)

        if MultiDigV == false then
            MultiDigV = bool
            UserInputService.InputBegan:Connect(
                function(key)
                    if key.UserInputType == Enum.UserInputType.MouseButton1 and deb == false then
                        print("Multi Digged.")

                        for i = 25, 0, -1 do
                            player.Character:FindFirstChildOfClass("Tool").ToolRemote:FireServer(unpack(args))
                        end

                        deb = true
                        wait(0.5)
                        deb = false
                    else
                        warn("Cooldown...")
                    end
                end
            )
        else
            print("Multi Dig Disabled")
            MultiDigV = false
        end
    end

    end)
